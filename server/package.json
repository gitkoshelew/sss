{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Server side rendering project",
  "main": "index.js",
  "scripts": {
    "dev-reload:build-client-watch": "set NODE_ENV=production&&set REBUILD_MODE=true&& webpack --mode production --env.NODE_ENV=production  --config webpack.client.js --watch",
    "dev-reload:build-server-watch": "set NODE_ENV=production&&set REBUILD_MODE=true&& webpack --mode production --env.NODE_ENV=production  --config webpack.server.js --watch",
    "dev-reload:demon-server": "nodemon --watch build/server/ --ignore build/server/assets/ --delay 1 --exec node ./build/server/bundle.js",
    "dev:server": "set NODE_ENV=development&& webpack --mode development --env.NODE_ENV=development  --config webpack.server.js",
    "dev:client": "set NODE_ENV=production&&set PREBUILD=true&& webpack --mode production --env.NODE_ENV=production --config webpack.client.js",
    "build:client": "set NODE_ENV=production&& webpack --mode production --env.NODE_ENV=production --config webpack.client.js -p",
    "build:server": "set NODE_ENV=production&& webpack --mode production --env.NODE_ENV=production --config webpack.server.js -p ",
    "run-server": "node ./build/server/bundle.js",
    "webpack-devserver": "set NODE_ENV=development&& webpack-dev-server --mode development  --env.NODE_ENV=development --config webpack.client.js --output-pathinfo --debug --hot --devtool eval-source-map --colors --inline --open --port 8040 --host 0.0.0.0",
    "stop-server": "killall node",
    "start-devreload": "npm-run-all --parallel dev-reload:*",
    "start-dev": "npm-run-all --serial dev:client dev:server --parallel run-server webpack-devserver",
    "build": "npm-run-all --serial build:*"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.6.0",
    "assets-webpack-plugin": "^3.9.10",
    "axios": "0.18.1",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-named-asset-import": "^0.3.4",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-app": "^9.0.2",
    "babel-preset-stage-0": "6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "compression": "^1.7.4",
    "concurrently": "3.5.0",
    "copy-webpack-plugin": "^5.0.5",
    "core-js": "^3.4.5",
    "css-loader": "^3.2.0",
    "eslint": "^6.7.1",
    "eslint-loader": "^3.0.2",
    "express": "^4.17.1",
    "express-http-proxy": "1.0.6",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^5.0.2",
    "json-loader": "^0.5.7",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lodash": "4.17.13",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "nodemon": "1.12.0",
    "npm-run-all": "4.1.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "7.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.1",
    "react": "16.0.0",
    "react-dev-utils": "^9.1.0",
    "react-dom": "16.0.1",
    "react-helmet": "5.2.0",
    "react-redux": "5.0.6",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^8.0.0",
    "serialize-javascript": "1.4.0",
    "style-loader": "^1.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^3.0.0",
    "webpack": "4.39.1",
    "webpack-merge": "4.1.0",
    "webpack-node-externals": "1.6.0"
  },
  "devDependencies": {
